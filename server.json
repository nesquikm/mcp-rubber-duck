{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
  "name": "io.github.nesquikm/rubber-duck",
  "description": "Bridge to multiple LLMs and CLI agents: councils, debates, voting, and more",
  "repository": {
    "url": "https://github.com/nesquikm/mcp-rubber-duck",
    "source": "github"
  },
  "version": "1.14.3",
  "packages": [
    {
      "registryType": "npm",
      "identifier": "mcp-rubber-duck",
      "version": "1.14.3",
      "transport": {
        "type": "stdio"
      },
      "environmentVariables": [
        {
          "description": "OpenAI API key (starts with sk-)",
          "format": "string",
          "isSecret": true,
          "name": "OPENAI_API_KEY"
        },
        {
          "description": "Google Gemini API key",
          "format": "string",
          "isSecret": true,
          "name": "GEMINI_API_KEY"
        },
        {
          "description": "Groq API key (starts with gsk_)",
          "format": "string",
          "isSecret": true,
          "name": "GROQ_API_KEY"
        },
        {
          "description": "Default LLM provider to use",
          "format": "string",
          "name": "DEFAULT_PROVIDER"
        }
      ]
    }
  ]
}